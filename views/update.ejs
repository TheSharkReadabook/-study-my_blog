<form name="update" method="post" action="/post/update/<%= data.postid %>?_method=PUT">
    <input type="hidden" name="_method" value="put"/>

    <table border="1">
          <tr>
              <td>id</td>
              <td><input type='text' name='postid' id='postid' value='<%= data.postid %>'></td>
          </tr>
          <tr>
              <td>title</td>
              <td><input type='text' name='title' id='title' value='<%= data.title %>'></td>
          </tr>
          <tr>
              <td>content</td>
              <td><input type='text' name='content' id='content' value='<%= data.content %>'></td>
          </tr>
          <tr>
              <td>
                  <input type="submit" value="submit!">
              </td>
          </tr>
    </table>
</form>





